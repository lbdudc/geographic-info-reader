{
  "name": "@lbdudc/gp-shapefile-reader",
  "version": "1.1.1",
  "homepage": "https://github.com/lbdudc/shapefile-reader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbdudc/shapefile-reader.git"
  },
  "description": "Shapefile reader for Node.js, reads a folder and returns a JSON with the data of the shapefiles",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "example": "node ./examples/BasicExample.js",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "eslint": "eslint --ext js src examples",
    "eslint:fix": "eslint --ext js --fix src examples",
    "lint": "npm-run-all eslint prettier",
    "lint:fix": "npm-run-all eslint:fix prettier:fix",
    "prettier": "prettier --check --ignore-unknown src examples",
    "prettier:fix": "prettier --write --ignore-unknown src examples",
    "prepare": "husky install"
  },
  "dependencies": {
    "jschardet": "3.0.0",
    "jszip": "^3.10.1",
    "shapefile": "^0.6.6"
  },
  "author": "victor.lamas@udc.es",
  "license": "MIT",
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,html,yml}": "prettier --write"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^0.32.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "vitest": "^0.32.0"
  }
}
